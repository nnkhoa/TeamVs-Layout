<lineEdit id="pComm13" x="80" y="330" width="180" height="20" dataSet="3on3.csv" dataField="12" master="pTeamNo1">Comment 3</lineEdit>



<button type="swap" x="150" y="160" width="40" height="23" tooltip="Swap the Scores" id="swap1p12" swapSet1="pName11,pName11E,pBox11,pComm11" swapSet2="pName12,pName12E,pBox12,pComm12">Swap</button>
<button type="swap" x="150" y="260" width="40" height="23" tooltip="Swap the Scores" id="swap1p23" swapSet1="pName12,pName12E,pBox12,pComm12" swapSet2="pName13,pName13E,pBox13,pComm13">Swap</button>





