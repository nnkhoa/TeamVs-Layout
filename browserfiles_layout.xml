<lineEdit id="pName11E" x="80" y="110" width="180" height="20" dataSet="3on3.csv" dataField="5" master="pTeamNo1">Player 1 E</lineEdit>
<lineEdit id="pComm11" x="80" y="130" width="180" height="20" dataSet="3on3.csv" dataField="10" master="pTeamNo1">Comment 1</lineEdit>


<spinBox id="pBox12" x="25" y="190" width="40" height="22" maximum="1" />
