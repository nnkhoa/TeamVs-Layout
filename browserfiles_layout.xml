<!DOCTYPE StreamControlLayout>

<layout width="730" height="450" tabbed="1">

<tab name="3on3">
<label x="230" y="10" width="300" height="13">////////////////////3on3////////////////////</label>

<label x="5" y="34" width="77" height="13">Team 1P</label>
